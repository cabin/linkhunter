{
  "private": true,
  "name": "linkhunter",
  "version": "2.0.0",
  "author": "Zak Johnson <me@zakj.net>",
  "license": "MIT",
  "browserslist": "last 3 Chrome versions",
  "scripts": {
    "start": "rm -rf dist && webpack --progress --watch",
    "analyze": "webpack --env.analyze",
    "build": "rm -rf dist && webpack --env.production",
    "dist": "yarn build && zip -r linkhunter.zip dist"
  },
  "babel": {
    "compact": false,
    "presets": [
      "es2015"
    ],
    "plugins": [
      "lodash",
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "mixpanel",
    "parser": "babel-eslint",
    "env": {
      "node": true
    },
    "globals": {
      "chrome": true
    },
    "plugins": [
      "html"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "dependencies": {
    "animejs": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.11.1",
    "moment": "^2.18.1",
    "vue": "^2.3.3",
    "vue-focus": "^2.1.0",
    "vue-router": "^2.5.3",
    "vue-virtual-scroll-list": "^1.1.2",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-mixpanel": "^2.3.0",
    "eslint-plugin-html": "^3.1.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "js-beautify": "^1.6.14",
    "postcss-loader": "^2.0.5",
    "style-loader": "^0.18.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "vue-loader": "^13.0.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2"
  }
}
